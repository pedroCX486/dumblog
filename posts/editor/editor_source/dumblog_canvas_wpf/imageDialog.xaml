<Window x:Name="imageDialog" x:Class="dumblog_canvas_wpf.addImageDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dumblog_canvas_wpf"
        mc:Ignorable="d"
        Title="Add Image" Height="184.005" Width="366.161"
        ResizeMode="NoResize">
    <Grid RenderTransformOrigin="0.507,0.517">
        <Label x:Name="itemDescription" Content="Image URL:" HorizontalAlignment="Left" Margin="6,19,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="imageURL" HorizontalAlignment="Left" Height="23" Margin="10,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="330"/>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="63,100,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="OkButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="198,100,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" RenderTransformOrigin="0.518,0.451" Click="CancelButton_Click" Focusable="True" TabIndex="0"/>

    </Grid>
</Window>
